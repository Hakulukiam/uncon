{
    "defaults": {
        "native": {
            // Copy "statusBarBackgroundColor" and "splashscreenBackgroundColor" into config/app.json's "defaults.native" section
            "statusBarBackgroundColor": "#f2fcff",
            "splashscreenBackgroundColor": "#f2fcff",
            "ios": {
                // Copy bundleId into "defaults.native.ios" section
                "bundleId": "com.hero.crm"
            },
            "android": {
                // Copy packageName into "defaults.native.android" section
                "packageName": "com.hero.crm"
            }
        }
    }
}
